  <mode name="es-de" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="es-de.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="es-de.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="es-de.autobil.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-es-de.es-de.t1x"/>
        <file name="es-de.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-es-de.es-de.t2x"/>
        <file name="es-de.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-es-de.es-de.t3x"/>
        <file name="es-de.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="es-de.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

